@model IEnumerable<WebApplication.Models.DeviceType>
@{
    ViewData["Title"] = "IndexProperty";
}

<h1>Index Property</h1>

@if (Model.Count() > 0)
{
    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            @{
                int counter = 0;
                @foreach (var item in Model)
                {
                    counter++;
                }
            }
            @for (int i = 0; i < counter; i++)
            {
                <tr>i. Property Name</tr>
            }

        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td width="25%" align="center" valign="middle">@item.Name</td>
                </tr>
            }
        </tbody>

    </table>
}
else
{
    <p>No Devices Created Yet</p>
}