@model WebApplication.Models.ViewModels.DeviceViewModel
@{
    ViewData["Title"] = "Set";
}
<h1 class="text-primary">Add Device Type Properties</h1>
<br />
<div>
    
</div>
<form asp-action="Set">
    <input asp-action="DeviceType.Id" hidden />
    <div asp-validation-summary="ModelOnly" class="text-danger">
    </div>

    <div class="form-group">
        <input class=".add-more" type="button" value="+" />
    </div>
    <div id="textBoxContainer">
        
    </div>

    <div class="form-group row">
        <div class="col-8 offset-2 row">
            <div class="col">
                <button type="submit"  class="btn btn-primary w-75">Submit</button>
            </div>
            <div class="col">
                <a asp-action="Index" asp-controller="DeviceType" class="btn btn-secondary w-75">Back</a>
            </div>
        </div>
    </div>
</form>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>

